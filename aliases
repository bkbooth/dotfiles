alias adsf=asdf

# Git
alias gco="git checkout"
alias gs="git status"
alias gd="git diff"
alias gb="git branch"
alias gl="git log"
alias ga="git add"
alias gc="git commit"
alias gcm="gc -m"
alias gca="gc --amend"
alias gf="git fetch"
alias gfp="gf --prune"
alias gp="git pull"
alias gup="gp --rebase --autostash" # autostash requires git@2.9.0 or later

# Docker
alias dps="docker ps"
alias dpsa="dps -a"
alias drm="docker rm -fv \$(dpsa -q)"
alias di="docker images"
alias did="di --filter 'dangling=true'"
alias drmi="docker rmi"
alias drmid="drmi \$(did -q)"
alias dcb="docker-compose build"
alias dcu="docker-compose up"

# Ember
alias es="ember serve"
alias et="ember test"
alias ets="et --server"
